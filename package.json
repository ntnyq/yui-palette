{
  "name": "yui-palette",
  "version": "0.0.1",
  "description": "A copy of yui540 for ts and react learning.",
  "main": "webpack.config.js",
  "scripts": {
    "build:dev": "cross-env MODE=development webpack",
    "build:prod": "cross-env MODE=production webpack",
    "build": "npm-run-all build:prod",
    "watch": "cross-env MODE=development webpack --watch",
    "serve": "cross-env MODE=development webpack-dev-server",
    "fmt": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [ "prettier --write", "git add" ]
  },
  "repository": "https://github.com/ntnyq/yui-palette",
  "author": "ntnyq, ntnyq13@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/styled-components": "4.1.6",
    "awesome-typescript-loader": "5.2.1",
    "cross-env": "5.2.0",
    "file-loader": "3.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.1",
    "source-map-loader": "0.2.4",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.1.1",
    "typescript": "3.2.4",
    "url-loader": "1.1.2",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "4.3.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-router-dom": "4.3.1",
    "styled-components": "4.1.3"
  }
}
