{
  "name": "yui-palette",
  "version": "0.0.1",
  "description": "A copy of yui540 for ts and react learning.",
  "main": "webpack.config.js",
  "scripts": {
    "build:dev": "cross-env MODE=development webpack",
    "build:prod": "cross-env MODE=production webpack",
    "build": "npm-run-all build:prod",
    "watch": "cross-env MODE=development webpack --watch",
    "serve": "cross-env MODE=development webpack-dev-server",
    "pretty": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": "prettier --write"
  },
  "prettier": "prettier-config-goy",
  "repository": "https://github.com/ntnyq/yui-palette",
  "author": "ntnyq, ntnyq13@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/styled-components": "5.1.1",
    "cross-env": "7.0.2",
    "file-loader": "6.0.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-config-goy": "^1.1.0",
    "source-map-loader": "1.0.1",
    "svg-sprite-loader": "5.0.0",
    "svgo": "1.3.2",
    "ts-loader": "^8.0.0",
    "typescript": "3.9.6",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "styled-components": "5.1.1"
  }
}
