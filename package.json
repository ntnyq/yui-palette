{
  "name": "yui-palette",
  "version": "0.0.1",
  "description": "A copy of yui540 for ts and react learning.",
  "main": "webpack.config.js",
  "scripts": {
    "build:dev": "cross-env MODE=development webpack",
    "build:prod": "cross-env MODE=production webpack",
    "build": "npm-run-all build:prod",
    "watch": "cross-env MODE=development webpack --watch",
    "serve": "cross-env MODE=development webpack-dev-server",
    "fmt": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/ntnyq/yui-palette",
  "author": "ntnyq, ntnyq13@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/styled-components": "4.1.18",
    "awesome-typescript-loader": "5.2.1",
    "cross-env": "5.2.0",
    "file-loader": "4.0.0",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "source-map-loader": "0.2.4",
    "svg-sprite-loader": "4.1.6",
    "svgo": "1.2.2",
    "typescript": "3.5.3",
    "url-loader": "2.0.1",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2"
  },
  "dependencies": {
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.1",
    "styled-components": "4.3.2"
  }
}
